    "standard": "^7.1.2",
    "static-server": "^2.0.2",
    "string-streamer": "1.0.0",
    "transform-loader": "^0.2.3",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "lib/**",
      "test/**",
      "types/**"
    ],
    "globals": [
      "describe",
      "it",
      "Util",
      "TreeNode",
      "beforeEach",
      "module",
